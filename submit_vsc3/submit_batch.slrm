#!/bin/sh

#SBATCH -J npt_ob 
#SBATCH -N 1
#SBATCH -p mem_0064
#SBATCH --tasks-per-node=16
#SBATCH --time=0:01:00

module purge
module load gcc/7.2 openmpi/3.0.1 python/2.7 boost/1.64.0
module load gsl/2.4


current_dir=$1
file_arr=( `cat $current_dir/batch_files.tmp` ) 
echo "submit new batch" 
for dir in "${file_arr[@]}" 
  do 
  cd $dir 
  echo $dir
  bash COMMITT.sh &
  cd .. 
  done
wait 
